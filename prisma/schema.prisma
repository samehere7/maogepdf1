model ChatMessages {
  id          String   @id
  userId      String
  document_id String
  content     String
  isUser      Boolean  @default(false)
  timestamp   DateTime @default(now())
  User        User     @relation(fields: [userId], references: [id])

  @@index([document_id])
  @@index([userId, document_id])
  @@index([userId])
  @@schema("public")
}

model PDF {
  id              String            @id
  name            String
  url             String
  size            Int
  uploadDate      DateTime          @default(now()) @db.Timestamptz(6)
  lastViewed      DateTime          @default(now()) @db.Timestamptz(6)
  userId          String
  contentType     String            @default("application/pdf")
  User            User              @relation(fields: [userId], references: [id], onDelete: Cascade, onUpdate: NoAction)
  document_chunks document_chunks[]

  @@schema("public")
}

model User {
  id           String         @id
  email        String         @unique
  name         String?
  image        String?
  plus         Boolean        @default(false)
  is_active    Boolean        @default(true)
  expire_at    DateTime?
  createdAt    DateTime       @default(now()) @db.Timestamptz(6)
  updatedAt    DateTime       @default(now()) @db.Timestamptz(6)
  ChatMessages ChatMessages[]
  PDF          PDF[]

  @@schema("public")
}

model chat_messages {
  id          String    @id @default(dbgenerated("(gen_random_uuid())::text"))
  user_id     String?   @db.Uuid
  document_id String?
  content     String
  is_user     Boolean?  @default(false)
  timestamp   DateTime? @default(now()) @db.Timestamptz(6)
  created_at  DateTime? @default(now()) @db.Timestamptz(6)
  pdfs        pdfs?     @relation(fields: [document_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
  users       users?    @relation(fields: [user_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@index([document_id], map: "idx_chat_messages_document_id")
  @@index([timestamp], map: "idx_chat_messages_timestamp")
  @@index([user_id], map: "idx_chat_messages_user_id")
  @@schema("public")
}

model document_chunks {
  id                String             @id
  documentId        String
  content           String
  pageNumber        Int
  chunkNumber       Int
  createdAt         DateTime           @default(now()) @db.Timestamptz(6)
  updatedAt         DateTime           @db.Timestamptz(6)
  PDF               PDF                @relation(fields: [documentId], references: [id], onDelete: Cascade)
  vector_embeddings vector_embeddings?

  @@index([documentId])
  @@schema("public")
}

model pdfs {
  id            String          @id @default(dbgenerated("(gen_random_uuid())::text"))
  name          String
  url           String
  size          Int
  upload_date   DateTime?       @default(now()) @db.Timestamptz(6)
  last_viewed   DateTime?       @default(now()) @db.Timestamptz(6)
  user_id       String?         @db.Uuid
  content_type  String?         @default("application/pdf")
  created_at    DateTime?       @default(now()) @db.Timestamptz(6)
  updated_at    DateTime?       @default(now()) @db.Timestamptz(6)
  chat_messages chat_messages[]
  users         users?          @relation(fields: [user_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@index([upload_date(sort: Desc)], map: "idx_pdfs_upload_date")
  @@index([user_id], map: "idx_pdfs_user_id")
  @@schema("public")
}

model plus {
  id         String    @id @db.Uuid
  is_paid    Boolean?  @default(true)
  paid_at    DateTime? @default(now()) @db.Timestamptz(6)
  plan       String?   @default("plus")
  expire_at  DateTime? @db.Timestamptz(6)
  pdf_count  Int?      @default(0)
  chat_count Int?      @default(0)
  users      users     @relation(fields: [id], references: [id], onDelete: NoAction, onUpdate: NoAction)

  @@schema("public")
}

model user_daily_quota {
  id         String    @id @db.Uuid
  pdf_count  Int?      @default(0)
  chat_count Int?      @default(0)
  quota_date DateTime? @default(dbgenerated("CURRENT_DATE")) @db.Date
  users      users     @relation(fields: [id], references: [id], onDelete: NoAction, onUpdate: NoAction)

  @@schema("public")
}

model user_profiles {
  id         String    @id @db.Uuid
  email      String?
  name       String?
  avatar_url String?
  plus       Boolean?  @default(false)
  is_active  Boolean?  @default(true)
  expire_at  DateTime? @db.Timestamptz(6)
  created_at DateTime? @default(now()) @db.Timestamptz(6)
  updated_at DateTime? @default(now()) @db.Timestamptz(6)
  users      users     @relation(fields: [id], references: [id], onDelete: NoAction, onUpdate: NoAction)

  @@schema("public")
}

model vector_embeddings {
  id              String          @id
  chunkId         String          @unique
  embedding       Bytes
  dimension       Int
  metadata        Json?
  createdAt       DateTime        @default(now()) @db.Timestamptz(6)
  updatedAt       DateTime        @db.Timestamptz(6)
  document_chunks document_chunks @relation(fields: [chunkId], references: [id], onDelete: Cascade)

  @@index([chunkId])
  @@schema("public")
}

enum aal_level {
  aal1
  aal2
  aal3

  @@schema("auth")
}

enum code_challenge_method {
  s256
  plain

  @@schema("auth")
}

enum factor_status {
  unverified
  verified

  @@schema("auth")
}

enum factor_type {
  totp
  webauthn
  phone

  @@schema("auth")
}

enum one_time_token_type {
  confirmation_token
  reauthentication_token
  recovery_token
  email_change_token_new
  email_change_token_current
  phone_change_token

  @@schema("auth")
}
